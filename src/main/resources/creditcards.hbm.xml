<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.p2.model.CreditCards" table = "creditcards">

      
      <id name = "cardID" type = "int" column = "cardid">
         <generator class="native"/>
      </id>
      
      <property name = "cardNum" column = "cardnum" type = "long"/>
      <property name = "holderID" column = "holderid" type = "int"/>
      <property name = "holderName" column = "holdername" type = "string"/>
      <property name = "expDate" column = "expdate" type = "string"/>
      <property name = "cvc" column = "cvc" type = "int"/>
      
      
   </class>

   
</hibernate-mapping>