<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.p2.model.OrderContents" table = "ordercontents">

      
      <id name = "itemID" type = "int" column = "ordercontentsid">
         <generator class="identity"/>
      </id>
      

      <property name = "item" column = "item" type = "string"/>
      <property name = "price" column = "price" type = "double"/>
      <property name = "quantity" column = "quantity" type = "int"/>
      <many-to-one class="com.p2.model.Orders"
      name="orders" cascade="all"/>
      
   </class>

   
</hibernate-mapping>